{
  "expo": {
    "name": "test.io",
    "slug": "test-reward",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.test.reward",
      "icon": "./assets/icon.ios.png",
      "buildNumber": "2",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to detect the currency, language and place for shopping.",
        "NSLocationAlwaysUsageDescription": "We use your location to detect the currency, language and place for shopping.",
        "NSCameraUsageDescription": "We use camera to capture QR code.",
        "NSPhotoLibraryUsageDescription": "We use photo library to choose your images.",
        "NSFaceIDUsageDescription": "We use FaceID to authenticate the application and transactions.",
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "citymapper",
          "uber",
          "lyft",
          "waze"
        ]
      },
      "config": {
        "googleMapsApiKey": "AIzaSyBXzqDhJBMbGaZAX5mzabOH13PumgSc2bk"
      }
    },
    "android": {
      "package": "com.test.reward",
      "versionCode": 2,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.test.reward",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBXzqDhJBMbGaZAX5mzabOH13PumgSc2bk"
        }
      }
    }
  }
}
